node n5 {
    type lanswitch
    network-config {
	hostname n5
	!
    }
    canvas c1
    iconcoords {554.0 361.0}
    labelcoords {554.0 385.0}
    interface-peer {e0 n6}
    interface-peer {e1 n2}
    interface-peer {e2 n4}
    interface-peer {e3 n3}
}

node n6 {
    type router
    model router
    network-config {
	hostname n6
	!
	interface eth2
	 ip address 10.0.2.1/24
	 ipv6 address 2001:2::1/64
	!
	interface eth0
	 ip address 10.0.0.1/24
	 ipv6 address 2001:0::1/64
	!
    }
    canvas c1
    iconcoords {554.0 266.0}
    labelcoords {554.0 298.0}
    interface-peer {eth0 n1}
    interface-peer {eth2 n5}
}

link l1 {
    nodes {n1 n6}
    bandwidth 0
}

link l3 {
    nodes {n6 n5}
    bandwidth 0
}

link l2 {
    nodes {n5 n2}
    bandwidth 0
}

link l4 {
    nodes {n5 n4}
    bandwidth 0
}

link l5 {
    nodes {n5 n3}
    bandwidth 0
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses no
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

